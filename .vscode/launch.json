{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_BACKTRACE": "full"
      },
      "runtimeExecutable": "/usr/bin/deno",
      "runtimeArgs": [
        "run",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229,
      "console": "integratedTerminal"
    },
    {
      "request": "launch",
      "name": "Launch with lldb",
      "type": "node",
      "program": "${workspaceFolder}/main.ts",
      "cwd": "${workspaceFolder}",
      "env": {
        "RUST_BACKTRACE": "full"
      },
      "runtimeExecutable": "/usr/bin/lldb",
      "runtimeArgs": [
        "--",
        "/usr/bin/deno",
        "run",
        "--inspect-wait",
        "--allow-all"
      ],
      "attachSimplePort": 9229,
      "console": "integratedTerminal"
    }
  ]
}
